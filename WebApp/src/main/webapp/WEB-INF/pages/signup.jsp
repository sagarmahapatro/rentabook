<div class="container-fluid">
		<div class="row-fluid">
			<div class="span12">
				<fieldset>
				<legend>Please Fill the Required fields</legend>
				<form class="form-horizontal" method="post" action='adduser' name="employeeForm" id="employeeForm">
					<div class="control-group">
						<label class="control-label">First Name</label>
						<div class="controls">
							<input type="text" name="firstName" id="firstName" title="First Name" value="">
						</div>
					</div>
					<div class="control-group">
						<label class="control-label">Last Name</label>
						<div class="controls">
							<input type="text" name="lastName" id="lastName" title="Last Name" value="">
						</div>
					</div>
					<div class="control-group">
						<label class="control-label">Email</label>
						<div class="controls">
							<input type="text" name="email" id="email" title="Email" value="">
						</div>
					</div>
					
					<div class="control-group">
						<label class="control-label">Enter Password</label>
						<div class="controls">
							<input type="text" name="password" id="password" title="password" value="">
						</div>
					</div>
					
					<div class="control-group">
						<label class="control-label">ReEnter Password</label>
						<div class="controls">
							<input type="text" name="repassword" id="repassword" title="repassword" value="">
						</div>
					</div>
					<div class="form-actions">
						<button type="submit" class="btn btn-success">Done</button>
					</div>
				</form>
				</fieldset>
			</div>
		</div>
	</div>	
	<p><a href="home">Go back to Homepage</a></p>	